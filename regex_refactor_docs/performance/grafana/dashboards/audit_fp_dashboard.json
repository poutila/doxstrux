{
  "dashboard": {
    "title": "Audit Issue Automation - FP Telemetry",
    "tags": ["audit", "security", "automation"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "1m",
    "panels": [
      {
        "id": 1,
        "title": "Issue Creation Failures (1h)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "increase(audit_issue_create_failures_total[1h])",
            "refId": "A",
            "legendFormat": "Failures"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "red"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "title": "Audit Digests Created (24h)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "increase(audit_digest_created_total[24h])",
            "refId": "A",
            "legendFormat": "Digests"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 3, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "title": "False Positive Rate (7d)",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "(increase(audit_fp_marked_total[7d]) / max(increase(audit_unregistered_repos_total[7d]), 1)) * 100",
            "refId": "A",
            "legendFormat": "FP Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 10, "color": "red"}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 4,
        "title": "Repos Scanned (24h)",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "increase(audit_unregistered_repos_total[24h])",
            "refId": "A",
            "legendFormat": "Repos"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "title": "Issue Creation Failures Over Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "increase(audit_issue_create_failures_total[5m])",
            "refId": "A",
            "legendFormat": "Failures (5m)"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Failures"},
          {"format": "short", "show": false}
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "pointradius": 2,
        "points": false,
        "bars": false,
        "stack": false,
        "percentage": false,
        "legend": {
          "show": true,
          "values": false,
          "min": false,
          "max": false,
          "current": false,
          "total": false,
          "avg": false,
          "alignAsTable": false,
          "rightSide": false
        },
        "nullPointMode": "null",
        "tooltip": {"shared": true, "sort": 0, "value_type": "individual"}
      },
      {
        "id": 6,
        "title": "FP Rate Trend (7d window)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "(increase(audit_fp_marked_total[7d]) / max(increase(audit_unregistered_repos_total[7d]), 1)) * 100",
            "refId": "A",
            "legendFormat": "FP Rate %"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "FP Rate", "min": 0, "max": 100},
          {"format": "short", "show": false}
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "pointradius": 2,
        "points": false,
        "bars": false,
        "stack": false,
        "percentage": false,
        "legend": {
          "show": true,
          "values": false,
          "min": false,
          "max": false,
          "current": false,
          "total": false,
          "avg": false,
          "alignAsTable": false,
          "rightSide": false
        },
        "nullPointMode": "null",
        "tooltip": {"shared": true, "sort": 0, "value_type": "individual"}
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    }
  }
}
